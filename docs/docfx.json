{
  "metadata":[
    {
      "src": [
        {
          "files": ["Dangl.Data.Shared.csproj"],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/Dangl.Data.Shared"
        }
      ],
      "dest": "shared",
      "properties": {
          "TargetFramework": "netstandard1.0"
      }
    },
	{
      "src": [
        {
          "files": ["Dangl.Data.Shared.AspNetCore.csproj"],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/Dangl.Data.Shared.AspNetCore"
        }
      ],
      "dest": "shared-aspnetcore"
    }
  ],
  "build":{
    "content":[
      {
        "files":[
          "*.md",
          "**/*.yml"
        ]
      }
    ],
    "resource":[
      {
        "files":[
          "**.png"
        ]
      }
    ],
    "overwrite":"specs/*.md",
    "globalMetadata":{
      "_appTitle":"Dangl.Data.Shared Documentation",
      "_appFooter":"&copy; <a href=\"https://www.dangl-it.com\" target=\"_blank\">Dangl IT - Georg Dangl</a>",
      "_appLogoPath":"app-logo.png",
      "_appFaviconPath":"app-logo.png",
      "_enableSearch":true,
      "_disableContribution":true
    },
    "dest":"../generated_docs"
  }
}
